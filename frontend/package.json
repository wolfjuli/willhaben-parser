{
    "name": "frontend",
  "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "check": "pnpm run /^check:.*/",
        "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:test": "pnpm run test --run",
        "dev": "pnpm run /^dev:.*/",
        "dev:vite": "svelte-kit sync && vite dev",
        "fix": "prettier --write --ignore-path ../.gitignore --ignore-path ./.prettierignore . && eslint . --fix",
        "prepare": "svelte-kit sync || echo ''",
        "test": "vitest"
    },
    "dependencies": {
        "@picocss/pico": "^2.0.6",
      "@neodrag/svelte": "^2.3.2",
        "bootstrap": "^5.3.3",
        "bootstrap-grid": "^3.0.1",
        "bootstrap-grid-flexbox": "^1.1.2",
        "i": "^0.3.7",
        "npm": "^11.1.0",
        "vitest": "^3.0.0"
    },
    "devDependencies": {
        "@awmottaz/prettier-plugin-void-html": "^1.8.0",
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@poppanator/sveltekit-svg": "^5.0.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/svelte": "^5.2.4",
        "@types/google.maps": "^3.58.1",
        "@vitest/ui": "3.0.5",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^2.46.1",
        "globals": "^15.14.0",
        "jsdom": "^25.0.1",
        "svelte": "^5.20.0",
        "svelte-check": "^4.0.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.20.0",
        "vite": "^6.0.0",
        "vite-plugin-circular-dependency": "^0.5.0"
    },
    "packageManager": "pnpm@10.3.0+sha512.ee592eda8815a8a293c206bb0917c4bb0ff274c50def7cbc17be05ec641fc2d1b02490ce660061356bd0d126a4d7eb2ec8830e6959fb8a447571c631d5a2442d",
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}
